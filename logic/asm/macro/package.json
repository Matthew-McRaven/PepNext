    "@pepnext/eslint": "^3.0.3",
    "@pepnext/tsconfig": "^3.0.0",
    "@types/jest": "^27.4.0",
    "jest": "^27.3.1",
    "jest-junit": "^13.0.0",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.0.7",
    "typescript": "^4.8.3"
  },
  "packageManager": "npm@6.14.17",
  "publishConfig": {
    "@pepnext:registry": "https://gitlab.com/api/v4/projects/39773348/packages/npm/"
